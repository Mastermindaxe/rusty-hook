{
  "files.exclude": {
    "**/target/": true,
    "**/*.rs.bk": true,
    ".coverage": true,
    ".testresults": true,
    "**/kcov*/": true,
    "**/*.gcda": true,
    "**/*.gcno": true
  },
  "cSpell.enabled": true,
  "cSpell.ignorePaths": [
    "Cargo.lock",
    "**/.git/**",
    ".vscode",
],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "azure-pipelines",
    "html",
    "json",
    "markdown",
    "plaintext",
    "powershell",
    "properties",
    "restructuredtext",
    "rust",
    "text",
    "toml",
    "yaml",
    "yml"
  ],
  "cSpell.dictionaries": [
    "companies",
    "cpp",
    "en_us",
    "filetypes",
    "misc",
    "powershell",
    "rust",
    "softwareTerms"
  ],
  "vsicons.associations.files": [
    {
      "icon": "azurepipelines",
      "extensions": [
        "azure-pipelines.linux-pr.yml",
        "azure-pipelines.mac-pr.yml",
        "azure-pipelines.windows-pr.yml",
        "azure-pipelines.ci.yml",
      ],
      "filename": true,
      "format": "svg"
    }
  ],
  "files.associations": {
    "*azure-pipelines*": "azure-pipelines"
  }
}
